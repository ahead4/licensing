{
    "name": "ahead4/licensing",
    "description": "A package to assist with the generation and validation of license keys.",
    "authors": [
        {
            "name": "Ahead4 Limited",
            "email": "web@ahead4.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Ahead4\\Licensing\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}